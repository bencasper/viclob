{% extends "home/base.html" %}
{% load viclob_tags wagtailcore_tags static %}


{% block content %}


<!-- Page Content -->
<div class="container">

    <!-- Page Heading/Breadcrumbs -->
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">{{ self}}
            </h1>
            {% breadcrumbs %}
        </div>
    </div>
    <!-- /.row -->

    <!-- /.row -->

    <!-- Content Row -->
    <div class="row">
        <!-- Map Column -->
        <div class="col-md-8">
            <!-- Embedded Google Map -->
            <iframe width="100%" height="400px" frameborder="0" scrolling="no" marginheight="0" marginwidth="0"
                    src="http://maps.google.com/maps?hl=en&amp;ie=UTF8&amp;ll=37.0625,-95.677068&amp;spn=56.506174,79.013672&amp;t=m&amp;z=4&amp;output=embed"></iframe>
        </div>
        <!-- Contact Details Column -->
        <div class="col-md-4">
            <h3>联系我们</h3>
            <p><i class="fa fa-adn"></i>
                地址<br><br>
            </p>
            <p><i class="fa fa-phone"></i>
                电话: (123) 456-7890</p>
            <p><i class="fa fa-envelope-o"></i>
                Email: <a href="mailto:name@example.com">name@example.com</a>
            </p>
            <p><i class="fa fa-weixin"></i>
                微信: Monday - Friday: 9:00 AM to 5:00 PM</p>

        </div>
    </div>
    <!-- /.row -->


</div>
<!-- /.container -->

{% endblock %}
